{
  "$schema": "https://raw.githubusercontent.com/shikijs/tm-themes/master/schema.json",
  "name": "jaywalkin-coffee-theme",
  "type": "dark",
  "colors": {
    "editor.background": "#3E2723",
    "editor.foreground": "#fff2c5"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#8D8F8C",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "string",
      "settings": { "foreground": "#8D8F8C" }
    },
    {
      "scope": [
        "string.quoted.docstring.multi.python",
        "string.quoted.triple.python"
      ],
      "settings": { 
        "foreground": "#8D8F8C", 
        "fontStyle": "italic" 
      }
    },
    {
      "scope": [
        "keyword.control",
        "keyword.operator.logical.python",
        "storage.type",
        "storage.modifier"
      ],
      "settings": { "foreground": "#E0A458" }
    },
    {
      "scope": "constant.language.boolean.python",
      "settings": { "foreground": "#E0A458" }
    },
    {
      "scope": "constant.numeric",
      "settings": { "foreground": "#E0A458" }
    },
    {
      "scope": "constant.language",
      "settings": { "foreground": "#E0A458" }
    },
    {
      "scope": [
        "variable.parameter.function.python",
        "variable.other.readwrite"
      ],
      "settings": { "foreground": "#5E7C7E" }
    },
    {
      "scope": "support.function.builtin",
      "settings": { "foreground": "#E0A458" }
    },
    {
      "scope": [
        "entity.name.function.definition",
        "meta.function-call"
      ],
      "settings": { "foreground": "#F4CAB8" }
    },
    {
      "scope": "support.namespace",
      "settings": { "foreground": "#8D8F8C" }
    },
    {
      "scope": [
        "entity.name.type.class.python",
        "support.type"
      ],
      "settings": { "foreground": "#fff2c5", "fontStyle": "underline" }
    },
    {
      "scope": "entity.name.decorator",
      "settings": { "foreground": "#E0A458", "fontStyle": "italic" }
    },
    {
      "scope": [
        "punctuation.definition.string",
        "punctuation.definition.comment",
        "punctuation.separator.key-value"
      ],
      "settings": { "foreground": "#8D8F8C" }
    },
    {
      "scope": "keyword.operator.arithmetic",
      "settings": { "foreground": "#8D8F8C" }
    }
  ]
}